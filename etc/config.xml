        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <catalog>
            <search>
                <elasticsearch7_cluster_index_prefix>magento2</elasticsearch7_cluster_index_prefix>
                <elasticsearch7_cluster_enable_auth>0</elasticsearch7_cluster_enable_auth>
